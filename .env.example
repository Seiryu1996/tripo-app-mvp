# Database
DATABASE_URL="mysql://username:password@host:4000/database_name?sslaccept=strict&sslmode=require"

# NextAuth.js
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-key"

# Admin credentials
ADMIN_EMAIL="admin@example.com"
ADMIN_PASSWORD="admin123"

# Tripo API
TRIPO_API_KEY="your-tripo-api-key"
TRIPO_API_URL="https://api.tripo3d.ai/v2/openapi"

# Google Cloud Storage
GCS_BUCKET_NAME="your-model-bucket"
GCP_PROJECT_ID="your-gcp-project-id"
GCP_CLIENT_EMAIL="service-account@example.iam.gserviceaccount.com"
# Replace literal newlines in the private key with \n before pasting
GCP_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\\nYOUR_PRIVATE_KEY\\n-----END PRIVATE KEY-----\\n"

# File upload settings
NEXT_PUBLIC_MAX_FILE_SIZE="10485760"
NEXT_PUBLIC_ALLOWED_FILE_TYPES="image/jpeg,image/png,image/webp"
